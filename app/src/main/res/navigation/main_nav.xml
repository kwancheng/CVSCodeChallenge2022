<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.gk.cvscodechallenge.ui.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_TSOFragment"
            app:destination="@id/TSOFragment" />
    </fragment>
    <fragment
        android:id="@+id/TSOFragment"
        android:name="com.gk.cvscodechallenge.ui.DetailsFragment"
        android:label="fragment_tso"
        tools:layout="@layout/fragment_details" />
</navigation>